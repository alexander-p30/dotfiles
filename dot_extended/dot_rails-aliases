alias rs="rails s"
alias rc="rails c"
alias rdbr="rails db:reset"
alias rdbm="rails db:migrate"
alias rdbset="rails db:setup"
alias rdbd="rails db:drop"
alias rdbc="rails db:create"
alias rdbs="rails db:seed"
alias rdbrs="rails db:drop && rails db:create && rails db:migrate && rails db:seed"
alias rg="rails generate"
alias rgc="rails generate controller"
alias rgm="rails generate model"
alias rgmi="rails generate migration"

